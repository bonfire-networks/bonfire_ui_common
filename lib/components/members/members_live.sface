<table class="table w-full table-zebra">
  {!-- head --}
  <thead>
    <tr>
      <th>{l("Name")}</th>
      <th>{l("Role")}</th>
      <th>{l("Status")}</th>
      <th>{l("Signed up")}</th>
      <th> </th>
    </tr>
  </thead>
  <tbody>
    {!-- row 1 --}
    {#for {user, index} <- Enum.with_index(@users)}
      <Bonfire.UI.Common.MemberLive user={user} is_local_user={@is_local_users} counter={index + 1} />
    {/for}
  </tbody>
</table>
